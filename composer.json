{
  "name": "mridang/magazine",
  "description": "Magazine helps you automatically package extensions for Magento 1.x",
  "keywords": [
    "package",
    "magento"
  ],
  "homepage": "https://github.com/mridang/magazine",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mridang Agarwalla",
      "email": "mridang.agarwalla+magazine@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/mridang/magazine/issues"
  },
  "require": {
    "php": "^5.3.2 || ^7.0",
    "symfony/console": "^2.5 || ^3.0"
  },
  "config": {
    "platform": {
      "php": "5.3.9"
    }
  },
  "autoload": {
    "psr-4": {
      "Magazine\\": "src/Magazine"
    }
  },
  "bin": [
    "bin/magazine"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
